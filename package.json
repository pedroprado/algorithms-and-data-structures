{
  "name": "algorithms",
  "version": "0.0.1",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "jest": "^26.0.1"
  },
  "scripts": {
    "test": "jest --watchAll=false"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverageFrom": [
      "/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "lcov",
      "text"
    ]
  }
}
